{
  "name": "build-version-plugin",
  "version": "0.1.1",
  "description": "build version plugin",
  "browser": "dist/webpack-plugin.js",
  "module": "dist/webpack-plugin.mjs",
  "unpkg": "dist/webpack-plugin.min.js",
  "jsdelivr": "dist/webpack-plugin.min.js",
  "exports": {
    ".": {
      "import": "./dist/webpack-plugin.mjs",
      "require": "./dist/webpack-plugin.js"
    },
    "./package.json": "./package.json"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "webpack-plugin",
    "browser",
    "vue",
    "vue-cli",
    "client",
    "build-version",
    "browserify"
  ],
  "scripts": {
    "test": "grunt test",
    "format": "grunt exec:format",
    "dist": "rm -rf dist/* && rollup -c",
    "build": "rollup -c",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iwlking/build-version-plugin.git"
  },
  "files": [
    "index.js",
    "dist/**/*"
  ],
  "author": "wlking",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "browserstack-runner": "^0.9.0",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-promise": "^5.1.0",
    "grunt": "^1.0.4",
    "grunt-compare-size": "^0.4.2",
    "grunt-contrib-connect": "^3.0.0",
    "grunt-contrib-nodeunit": "^4.0.0",
    "grunt-contrib-qunit": "^3.1.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-exec": "^3.0.0",
    "gzip-js": "^0.3.2",
    "prettier": "^2.3.2",
    "qunit": "^2.9.3",
    "release-it": "^14.10.0",
    "rollup": "^2.68.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-license": "^2.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.3"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1"
  }
}
